{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2018", "dom"],
    "jsx": "preserve",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noEmit": true,
    "alwaysStrict": true,
    "typeRoots" : ["./src/js/types", "./cypress/types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["cypress"]
  }
}
